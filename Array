#include using namespace std;

struct Student { int id; string name; string email; };

int main() { Student student[5];

for (int i = 0; i < 5; i++) {
    cout << "Enter ID for student " << i+1 << ": ";
    cin >> student[i].id;

    cout << "Enter Name for student " << i+1 << ": ";
    cin >> student[i].name;  

    cout << "Enter Email for student " << i+1 << ": ";
    cin >> student[i].email;

    cout << endl;
}

cout << "\n Student List \n";
for (int i = 0; i < 5; i++) {
    cout << "ID: " << student[i].id
         << " Name: " << student[i].name
         << " Email: " << student[i].email << endl;
}

return 0;
}
